<div class="container-fluid">
  <div class="row-fluid content-wrapper">
    <div class="span5">
      <form class="form-horizontal">

        <div class="control-group">
          <label class="control-label" for="inputUsername">Username</label>
          <div class="controls">
            <input type="text" ng-model="userName"> {{userName}}
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="inputEmail">Email</label>
          <div class="controls">
            <input type="text" ng-model="userEmail"> {{userEmail}}
          </div>
        </div>

        <div class="control-group">
          <label class="control-label" for="password">password</label>
          <div class="controls">
            <input type="password" ng-model="userPassword"> {{userPassword}}
          </div>
        </div>
        <div class="control-group">
          <label class="control-label" for="password_confirm">password_confirm</label>
          <div class="controls">
            <input type="password" ng-model="passwordConfirm"> {{passwordConfirm}}
          </div>
        </div>


      </form>
    </div>
    <div class="span5 border-util">

    </div>
  </div>
</div>


<%#= link_to 'New User', new_user_path %>
